    "sdk": {
      "type": "cosmos",
      "version": "v0.47.7"
    },
    "ibc": {
      "type": "go",
      "version": "v7.3.1"
    },
    "cosmwasm": {
      "version": "v0.44.0",
      "enabled": true
    },
    "tag": "v1.0.1"
  },
  "logo_URIs": {
