      {
        "address": "https://rpc.devnet-2.nibiru.fi",
        "provider": "Nibiru Foundation"
      }
    ],
    "rest": [
      {
        "address": "https://lcd.devnet-2.nibiru.fi",
        "provider": "Nibiru Foundation"
      }
    ],
    "grpc": [
      {
        "address": "grpc.devnet-2.nibiru.fi:443",
        "provider": "Nibiru Foundation"
      }
    ]
