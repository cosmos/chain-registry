{
  "$schema": "../chain.schema.json",
  "chain_name": "unification",
  "status": "live",
  "network_type": "mainnet",
  "website": "https://unification.com/",
  "pretty_name": "Unification",
  "chain_id": "FUND-MainNet-2",
  "bech32_prefix": "und",
  "daemon_name": "und",
  "node_home": "$HOME/.und_mainchain",
  "key_algos": [
    "secp256k1"
  ],
